{
  "name": "strapi-supercharged",
  "version": "0.4.0",
  "description": "Supercharge your Strapi applications and plugins with typescript compiler",
  "license": "MIT",
  "author": {
    "name": "digisquad.io",
    "url": "https://digisquad.io",
    "email": "contact@digisquad.io"
  },
  "keywords": [
    "strapi",
    "cms",
    "framework",
    "types",
    "plugins",
    "application",
    "typescript",
    "supercharged"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:digisquad-io/strapi-supercharged.git"
  },
  "types": "types",
  "files": [
    "types"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V"
  },
  "lint-staged": {
    "*.d.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write .",
    "test": "run-s test:*",
    "test:tsc": "tsc -p .",
    "test:eslint": "eslint .",
    "test:prettier": "prettier --check .",
    "commit": "commit",
    "release": "run-s test release:*",
    "release:standard-version": "standard-version"
  },
  "dependencies": {
    "@types/bookshelf": "^1.2.5",
    "@types/events": "3.0.0",
    "@types/koa": "2.13.4",
    "@types/koa-bodyparser": "4.3.3",
    "@types/koa-joi-router": "8.0.3",
    "@types/node": "16.11.6",
    "@types/pino": "6.3.12",
    "@types/request": "2.48.7"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@commitlint/prompt-cli": "13.2.1",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "commitlint": "13.2.1",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "lint-staged": "11.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "standard-version": "9.3.2",
    "typescript": "4.4.4",
    "yorkie": "2.0.0"
  }
}
